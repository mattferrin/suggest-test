{
  "name": "@clowd/suggest-test",
  "version": "1.0.0",
  "description": "A clover.xml parser to suggest the next unit test to write",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "fsl": "node node_modules/@clowd/fsl/dist/index.js",
    "jscpd": "jscpd --threshold=1 --reporters=html ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:w": "tsc -w"
  },
  "license": "MIT",
  "devDependencies": {
    "@clowd/fsl": "^1.0.4",
    "@types/node": "^18.11.9",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jscpd": "^3.5.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "husky": "^8.0.2",
    "meow": "^9.0.0",
    "xml2js": "^0.4.23"
  },
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "suggest-test": "dist/bin/ts/cli.js"
  }
}
