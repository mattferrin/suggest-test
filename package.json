{
  "name": "@clowd/suggest-test",
  "version": "0.0.1-rc.1",
  "description": "A clover.xml parser to suggest the next unit test to write",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/**/*.ts",
    "fsl": "node node_modules/@clowd/fsl/dist/index.js",
    "jest:w": "jest --watch",
    "jest": "jest",
    "jscpd": "jscpd --threshold=1 --reporters=html ./src",
    "prepare": "husky install",
    "pretty:c": "npx prettier --check src",
    "pretty": "npx prettier --write src",
    "st": "suggest-test --includeTested=false --includeStatements=false",
    "tsc:w": "tsc -w",
    "tsc": "tsc",
    "version:rc": "npm version prerelease --preid=rc --force"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@clowd/fsl": "^1.0.4",
    "@clowd/suggest-test": "^0.0.1-rc.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jscpd": "^3.5.1",
    "prettier": "2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "meow": "^9.0.0",
    "xml2js": "^0.4.23"
  },
  "files": [
    "dist/**",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "suggest-test": "dist/bin/ts/cli.js"
  }
}
